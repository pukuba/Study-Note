{
  "name": "study-note",
  "version": "1.0.0",
  "description": "Node.js GRPC Note",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_PATH=dist/ node dist/server/index.js & NODE_PATH=dist/ node dist/client/index.js",
    "build": "tsc -p .",
    "protogen": "make protogen",
    "dev": "NODE_PATH=src/ nodemon src/server/index.ts & NODE_PATH=src/ nodemon src/client/index.ts",
    "test": "pm2 install typescript && sleep 3 && NODE_PATH=src/ pm2 start ts-node -- -P tsconfig.json src/server/index.ts && sleep 5 && NODE_PATH=src/ ts-mocha -p ./tsconfig.json src/TEST/*.test.ts --exit && pm2 delete all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pukuba/Study-Note.git"
  },
  "author": "pukuba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pukuba/Study-Note/issues"
  },
  "homepage": "https://github.com/pukuba/Study-Note#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.8",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-protobuf": "^3.15.0",
    "grpc-tools": "^1.10.0",
    "grpc_tools_node_protoc_ts": "^5.0.1",
    "mocha": "^8.3.0",
    "mongodb": "^3.6.4",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.4",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.4",
    "@types/mongodb": "^3.6.8"
  }
}